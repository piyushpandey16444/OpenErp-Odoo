<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="sale_quotation_seq" model="ir.sequence">
            <field name="name">Sales Quotation</field>
            <field name="code">sale.quotation</field>
            <field name="prefix">SQ</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


         <record id="sale_agreement_seq1" model='ir.sequence'>
            <field name="name">Agreement</field>
            <field name="code">sale.agreement</field>
            <field name="prefix">SA</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
