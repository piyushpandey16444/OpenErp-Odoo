    <!--    Piyush: code for Email Description Menu in the settings==========================================-->

        <record id="view_email_description_form" model="ir.ui.view">
            <field name="name">email.description.form</field>
            <field name="model">email.description</field>
            <field name="arch" type="xml">
                <form string="Email Description Details">
                    <group col="4" colspan="4">
                        <field name="migrate_data"/>
                        <field name="reference_id"/>
                        <field name="name"/>
                        <field name="subject"/>
                        <field name="email_description"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="view_email_description_tree" model="ir.ui.view">
            <field name="name">email.description.tree</field>
            <field name="model">email.description</field>
            <field name="arch" type="xml">
                <tree string="Email Description Details">
                    <field name="name"/>
                    <field name="subject"/>
                    <field name="email_description"/>
                    <field name="migrate_data" string="Migrate Data"/>
                    <field name="reference_id" string="Reference Id"/>
                </tree>
            </field>
        </record>

        <record id="action_email_description_form" model="ir.actions.act_window">
            <field name="name">Email Description</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">email.description</field>
