        <field name="arch" type="xml">

            <xpath expr="//label[@for='street']" position="before">
                    <field name="migrate_data" string="Migrate Data"/>
                </xpath>

            <xpath expr="//page[@name='sales_purchases']" position="after">
               <page name="migrate_info" string="Children" attrs="{'invisible': [('migrate_data', '=', False)]}">
                    <separator string="Ecom Integration"/>
                    <form>
                        <group col="2" colspan="4">
                            <field name="reference_id" readonly="1"/>
                        </group>
                    </form>
                    <field name="children_ids"/>
               </page>

               <page name="customer_cart" string="Customer Cart" attrs="{'invisible': [('migrate_data', '=', False)]}">
                    <separator string="Cart Items"/>
                    <field name="cart_ids"/>
               </page>

               <page name="customer_wishlist" string="Customer Wishlist" attrs="{'invisible': [('migrate_data', '=', False)]}">
                    <separator string="WishList Items"/>
                    <field name="wishlist_ids"/>
               </page>

             </xpath>
        </field>
    </record>


