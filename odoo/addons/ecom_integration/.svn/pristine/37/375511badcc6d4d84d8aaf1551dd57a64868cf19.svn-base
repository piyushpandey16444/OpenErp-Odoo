        <!--Piyush: code for creating view of Coupons in settings after images menu on 01-10-2020-->

    <record id="view_coupons_form" model="ir.ui.view">
            <field name="name">Coupons Form</field>
            <field name="model">available.coupons</field>
            <field name="arch" type="xml">
                <form string="Coupon Details">
                    <group col="4">
                        <field name="migrate_data" />
                        <field name="reference_id" />
                        <field name="name" />
                        <field name="coupon_description" />
                        <field name="valid_from" />
                        <field name="limit_number"/>
                        <field name="valid_to" />
                        <field name="minimum_amount" />
                        <field name="status" />
                        <field name="company_id" />
                    </group>
                    <hr/><hr/>
                    <group col="4" >
                        <field name="coupon_type" widget="radio" force_save="1"/>
                        <field name="allowed_users" widget="radio" force_save="1"/>
                        <field name="coupon_value"/>
                        <field name="partner_ids" widget="many2many_tags" attrs="{'invisible': [('allowed_users', '=', 'all')]}"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="view_coupons_tree" model="ir.ui.view">
            <field name="name">Coupons Tree</field>
            <field name="model">available.coupons</field>
            <field name="arch" type="xml">
                <tree string="Available Coupons Details">
                    <field name="name" />
                    <field name="coupon_type" />
                    <field name="coupon_value" />
                    <field name="valid_from" />
                    <field name="valid_to" />
                    <field name="limit_number" />
                    <field name="minimum_amount" />
                    <field name="company_id" />
                    <field name="status" />
                </tree>
            </field>
        </record>


