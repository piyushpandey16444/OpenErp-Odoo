    <record id="company_ecom_inherit_view" model="ir.ui.view">
        <field name="name">company.ecom.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="ecom_company" options="{'no_create': True, 'no_open': True}"/>
