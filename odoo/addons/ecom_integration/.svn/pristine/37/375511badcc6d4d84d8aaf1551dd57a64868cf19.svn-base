                <field name="reference_id"/>
            </field>
        </field>
    </record>

    <!--    Piyush: code for Inheriting company fields ends here -->


    <!--    Piyush: code for Inheriting Product Category fields -->

    <record id="product_category_ecom_inherit_view" model="ir.ui.view">
        <field name="name">product.category.ecom.inherit</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <field name="name" position="before">
                <field name="image" widget="image" class="oe_avatar"/>
<!--                <field name="image" widget="image" class="oe_avatar" attrs="{'required': [('image_check', '=', True)]}"/>-->
            </field>
                <field name="parent_id" position="before">
                    <field name="reference_id" readonly="1"/>
                    <field name="migrate_data"/>
                    <field name="invisible_flag"/>
            </field>
        </field>
    </record>

    <!--    Piyush: code for Inheriting Product Category fields ends here -->


<!--    Piyush: code for Inheriting users fields -->


    <record id="users_ecom_inherit_view" model="ir.ui.view">
        <field name="name">users.ecom.inherit</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <field name="company_ids" position="before">
                <field name="reference_id"/>
                <field name="migrate_data"/>
            </field>
        </field>
    </record>

    <!--    Piyush: code for Inheriting users fields ends here -->


    <!--    Piyush: code for Inheriting Product fields -->

    <record id="product_ecom_inherit_view" model="ir.ui.view">
        <field name="name">product.ecom.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">

            <field name="type" position="before">
                <field name="migrate_data"/>
                <field name="ecommerce_check" invisible="1"/>
            </field>


            <xpath expr="//page[@name='notes']" position="after">
               <page name="migrate_info" string="Ecommerce" >
                    <separator string="Ecom Integration"/>
                    <form>
                        <group col="2" colspan="4">
                            <field name="reference_id" readonly="1"/>
                        </group>
                        <group col="4">
                            <field name="category_ids" widget="many2many_tags"/>
                            <field name="care_instruction_id"/>
                            <field name="age_ids" widget="many2many_tags"/>
                            <field name="color_ids" widget="many2many_tags"/>
                            <field name="product_fabric_id"/>
                            <field name="discount_price" string="Price After Discount"/>
                            <field name="related_products" widget="many2many_tags"/>
                            <field name="mrp_data"/>
                            <field name="mrp_discounted_data"/>
                            <!--Jatin added shiiping charges field-->
                            <field name="shipping_charge"/>
                        </group>
                    </form>
               </page>
               <page name="multiple_images" string="Related Images" attrs="{'invisible': [('ecommerce_check', '=', False)]}">
                    <separator string="Related Images"/>
                    <form>
                        <field name="image_ids" force_save="1" attrs="{'required': [('migrate_data', '=', True)]}">
                        </field>
                    </form>

               </page>
             </xpath>
        </field>
    </record>


    <!--    Piyush: code for Inheriting users fields ends here -->


