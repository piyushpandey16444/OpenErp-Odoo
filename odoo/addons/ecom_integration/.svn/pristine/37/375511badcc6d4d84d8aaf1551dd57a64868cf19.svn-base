    <record id="view_db_details_form" model="ir.ui.view">
            <field name="name">Database Details Form</field>
            <field name="model">db.details</field>
            <field name="arch" type="xml">
                <form string="Database Details">
                    <header>
                        <button name="migrate_all_data" attrs="{'invisible': [('migration_required', '=', True)]}" string="Migrate All" type="object" class="oe_highlight"/>
                        <button name="fetch_all_data" attrs="{'invisible': [('migration_required', '=', True)]}" string="Fetch data" type="object" class="oe_highlight"/>
                    </header>
                    <group>
                        <field name="application"/>
                        <field name="migrate_data" attrs="{'invisible': [('migration_required', '=', True)]}"/>
                    </group>
                    <group col="4" colspan="4">
                        <field name="ip_address"/>
                        <field name="port"/>
                        <field name="db_name" attrs="{'invisible': [('migration_required', '=', True)]}"/>
                        <field name="migration_required" invisible="1"/>
                    </group>
                    <group col="4" colspan="4">
<!--                        <field name="company_id" invisible="1"/>-->
                        <field name="username"  attrs="{'invisible': [('migration_required', '=', True)]}"/>
                        <field name="password" attrs="{'invisible': [('migration_required', '=', True)]}"/>
                    </group>

                </form>
            </field>
        </record>

        <record id="view_db_details_tree" model="ir.ui.view">
            <field name="name">Database Details Tree</field>
            <field name="model">db.details</field>
            <field name="arch" type="xml">
                <tree string="Database Details">
                    <field name="db_name" />
                    <field name="port" />
                    <field name="ip_address" />
<!--                    <field name="company_id" invisible="1"/>-->
                    <field name="username" />
                    <field name="password" />
                </tree>
            </field>
        </record>


    <record id="action_db_details_tree" model="ir.actions.act_window">
            <field name="name">Database Details Tree</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">db.details</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

