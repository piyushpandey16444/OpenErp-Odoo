            </field>

            <xpath expr="//tree//field[@name='additional_info']" position="before">
                <field name="age_id" attrs="{'column_invisible': [('parent.data_from_hoitymoppet', '=', False)]}"/>
                <field name="color_id" attrs="{'column_invisible': [('parent.data_from_hoitymoppet', '=', False)]}"/>
                <field name="custom_measure_master_id" attrs="{'column_invisible': [('parent.data_from_hoitymoppet', '=', False)]}"/>
                <button name="get_user_custom_measures" type="object" string="Custom User Measures"
                        icon="fa-list" attrs="{'invisible': [('custom_measure_master_id', '=', False)]}"/>
            </xpath>

            <xpath expr="//form//field[@name='additional_info']" position="before">
                <field name="age_id" attrs="{'column_invisible': [('parent.data_from_hoitymoppet', '=', False)]}"/>
                <field name="color_id" attrs="{'column_invisible': [('parent.data_from_hoitymoppet', '=', False)]}"/>
                <field name="custom_measure_master_id" attrs="{'column_invisible': [('parent.data_from_hoitymoppet', '=', False)]}"/>
            </xpath>

